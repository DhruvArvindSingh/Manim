{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "start:frontend": "cd apps/frontend && pnpm start",
    "start:socket": "cd apps/socket-server && pnpm start",
    "dev:frontend": "cd apps/frontend && pnpm dev",
    "dev:socket": "cd apps/socket-server && pnpm dev",
    "dev:python": "cd python_docker && docker-compose up",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@next/swc-wasm-nodejs": "^15.4.6",
    "@react-three/drei": "^10.7.2",
    "@react-three/fiber": "^9.3.0",
    "@types/three": "^0.179.0",
    "eslint-config-next": "^15.4.6",
    "next": "^15.4.6",
    "three": "^0.179.1"
  }
}
